<h3>Liste des fichiers</h3>
<% if (files) { %>
    <ul>
        <% files.forEach(file => { %>
            <li><a href="/download/<%= file.filename %>"><%= file.filename %></a> - <%= file.length %> octets</li>
        <% }) %>
    </ul>
<% } else { %>
    <p>Aucun fichier trouv√©.</p>
<% } %>




